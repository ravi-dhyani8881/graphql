extend type Query {
 findcoldByQuery(query: String, start: String, rows: String, filterField: String, filterQuery: String, sort: String, advanceField: String, advanceQuery: String, advance: String): coldResponse
}

type coldResponse {
  start: String
  row: String
  statusCode: Int
  totalRecords: Int
  advanced: [Advanced]
  records: [Recordscold]
}
     
type Recordscold {
ID: String
createdAt: String
updatedAt: String

}