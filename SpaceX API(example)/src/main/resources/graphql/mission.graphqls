extend type Query {
 findmissionByQuery(query: String, start: String, rows: String, filterField: String, filterQuery: String, sort: String, advanceField: String, advanceQuery: String, advance: String, field: String ): missionResponse
}

type missionResponse {
  start: String
  row: String
  statusCode: Int
  totalRecords: Int
  advanced: [Advanced]
  records: [Recordsmission]
}
     
type Recordsmission {
id: UUID
name: VARCHAR
details: VARCHAR


}
